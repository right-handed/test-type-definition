{
  "compilerOptions": {
    "declaration": true,
    "strictNullChecks": true,
    "target": "es5",
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "lib": ["es2015", "dom"],
    "rootDir": "/",
    "allowJs": false,
    "skipLibCheck": false,
    "skipDefaultLibCheck": false,
    "newLine": "lf"
  },
  "exclude": ["node_modules"],
}
